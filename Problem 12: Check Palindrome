#include <stdio.h>

int main() {
    int arr[100], n, smallest;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter the elements: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    smallest = arr[0];  // Initialize with the first element
    for (int i = 1; i < n; i++) {
        if (arr[i] < smallest) {
            smallest = arr[i];
        }
    }

    printf("Smallest element in array: %d\n", smallest);

    return 0;
}
